{

  "manifest_version": 2,
  "name": "Dark Theme on Whatsapp Web",
  "version": "1.0",

  "description": "Activates dark theme on Whatsapp Web.",

  "icons": {
    "48": "icons/icon48.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://web.whatsapp.com/*"],
      "js": ["src/whatsappdark.js"]
    }
  ],
  "author": "jpstrnisko",
  "homepage_url": "https://github.com/jpstrnisko/whatsappdark"
}
